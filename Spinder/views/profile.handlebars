<img src="{{user.img}}">
<h1>{{user.username}}</h1>
<h3>{{user.firstName}} {{user.lastName}}</h3>
<h4>{{user.location.city}}, {{user.location.country}}</h4>
<ol>Top Artists
    {{#each topArtists}}
        <li>{{this.name}}</li>
    {{/each}}
</ol>
<ol>Top Songs
    {{#each topSongs}}
        <li>{{this.name}}</li>
    {{/each}}
</ol>
<ol>Top Playlists
    {{#each playlists}}
        <li>{{this.name}}</li>
    {{/each}}
</ol>